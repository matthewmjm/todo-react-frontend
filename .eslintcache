(updatedTodo) => {\n    let todos = this.state.todos.map(todo => todo.id === updatedTodo.id ? updatedTodo : todo)\n    this.setState( { todos })\n    patchTodo(updatedTodo)\n  }\n\n  deleteTodo = (id) => {\n    let filtered = this.state.todos.filter(todo => todo.id !== id)\n    this.setState({\n      todos: filtered\n    })\n\n    deleteTodo(id)\n  }\n\n  signUp = (user) => {\n    return fetch(\"http://localhost:4000/users\", {\n      method: \"POST\",\n      headers: {\n          \"Accept\": \"application/json\",\n          \"Content-Type\": \"application/json\"\n      },\n      body: JSON.stringify({user})\n    })\n    .then(response => response.json())\n    .then(response => {\n      if(response.errors){\n        this.setState({alerts: response.errors})\n      }\n      else {\n        localStorage.setItem('token', response.token)\n        this.setState({\n          user: response.user,\n          alerts: [\"User successfully created!\"]\n        })\n      }\n    })\n  }\n\n  render(){\n    return (\n      <div className=\"App\">\n        <h1>üó£ Todo App üìù</h1>\n        <Switch>\n          <PrivateRoute\n            exact\n            path=\"/\" \n            component={Home}\n            submitAction={this.addTodo}\n            updateTodo={this.updateTodo}\n            deleteTodo={this.deleteTodo}\n            todos={this.state.todos}\n          />\n          <Route exact path=\"/signup\" render={(routerProps) => {\n            return <SignUpForm {...routerProps} signUp={this.signUp} alerts={this.state.alerts}/>} \n          }/>\n          <Redirect to=\"/\" />\n        </Switch>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/matthewmalecki/Flatiron/mod4/to-do-app/empryer/frontend-todo-react/src/components/TodoContainer.js",[],"/Users/matthewmalecki/Flatiron/mod4/to-do-app/empryer/frontend-todo-react/src/components/TodoItem.js",[],"/Users/matthewmalecki/Flatiron/mod4/to-do-app/empryer/frontend-todo-react/src/components/TodoForm.js",[],"/User